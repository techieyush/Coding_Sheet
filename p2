
import java.util.Scanner;

public class Ptrn2 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int row=1;
        int nst=5;
        while(row<=n)
        {
            int i=1;
            while(i<=nst)
            {
                System.out.print("*"+" ");
                i++;
            }
            System.out.println(" ");
            row++;
            nst--;
        }
    }
    
}
