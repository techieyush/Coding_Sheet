
public class MissingNumber {

    public static int missingNum(int nums[],int n)
    {
        int sum=(n*(n+1)/2);
        int totalSum=0;
        for(int i=0;i<nums.length;i++)
        {
            totalSum+=nums[i];
        }
        return sum-totalSum;

    }
    public static void main(String[] args) {
        int n=5;
        int nums[]={1,2,4,5};
        System.out.println(missingNum(nums,n));
    }
    
}
