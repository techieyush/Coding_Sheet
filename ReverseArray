public class reverseArray {


    public static void reverse(int nums[]){
        int i=0;
        int j=nums.length-1;
        while(i<j)
        {
            int temp=nums[i];
            nums[i++]=nums[j];
            nums[j--]=temp;
        }
    }
	public static void main(String[] args) {
	int nums[]={1,2,3,4,5};
	reverse(nums);
	for(int i=0;i<nums.length;i++)
	{
	    System.out.print(nums[i]+" ");
	}
	}
}

    

